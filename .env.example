# OLMoCR via DeepInfra (default provider)
DEEPINFRA_API_KEY=<your-deepinfra-api-key>

# DeepSeek-OCR via Clarifai
CLARIFAI_PAT=<your-clarifai-personal-access-token>

# Self-hosted vLLM (optional, for local DeepSeek-OCR)
VLLM_API_KEY=<optional-key-for-secured-vllm-server>

# Custom OpenAI-compatible endpoint (optional)
CUSTOM_API_KEY=<your-custom-api-key>
CUSTOM_OCR_ENDPOINT=<your-custom-endpoint-url>
CUSTOM_OCR_MODEL=<your-custom-model-name>